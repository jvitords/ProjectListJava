package application;

import java.util.ArrayList;
import java.util.List;

public class Main {

	public static void main(String[] args) {
		
		List<Integer> numeros = new ArrayList<>();
		
		numeros.add(3);
		numeros.add(5);
		numeros.add(4);
		numeros.add(7);
		numeros.add(2, 19);
		
		for (Integer num : numeros) {
			System.out.println(num);
		}
	}

}
